def sbox_proj(c):
    s = c.proj(AES_SBOX, 8)
    return s

def sub_bytes(state):
    for i in range(16):
        #start_timer(1)
        state[i] = sbox_proj(state[i])
        #stop_timer(1)
    return state

SHIFT_ROWS_PERMUTATION = [0,5,10,15,4,9,14,3,8,13,2,7,12,1,6,11]
def shift_rows(state):
    return [state[SHIFT_ROWS_PERMUTATION[i]] for i in range(16)]

