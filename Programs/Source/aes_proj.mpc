def mix_columns(state):
    doubles = [x.proj(DOUBLE_TT, 8) for x in state]
    new_state = [None] * 16
    for i in range(0,16,4):
        new_state[i] = doubles[i] ^ doubles[i+1] ^ state[i+1] ^ state[i+2] ^ state[i+3]
        new_state[i+1] = state[i] ^ doubles[i+1] ^ doubles[i+2] ^ state[i+2] ^ state[i+3]
        new_state[i+2] = state[i] ^ state[i+1] ^ doubles[i+2] ^ doubles[i+3] ^ state[i+3]
        new_state[i+3] = doubles[i] ^ state[i] ^ state[i+1] ^ state[i+2] ^ doubles[i+3]
    return new_state

def add_round_key(state, key):
    for i in range(16):
        state[i] = state[i] ^ key[i]
    return state

def check_dim2(x, d0, n):
    assert len(x) == d0
    assert all([c.single_wire_n == n for c in x])

def check_dim3(x, d0, d1, n):
    assert len(x) == d0
    assert all([len(c) == d1 for c in x])
    assert all([c.single_wire_n == n for s in x for c in s])

ROUND_CONSTANTS = [0x01, 0x02, 0x04, 0x08, 0x10, 0x20, 0x40, 0x80, 0x1b, 0x36, 0x6c]

def expand_key(key):
    def update(key, r):
        new_key = [None] * 16
        for i in range(4):
            x = key[12+((i+1)%4)]
            x = sbox_proj(x)
            if i == 0:
                x ^= ROUND_CONSTANTS[r-1]
            new_key[i] = x ^ key[i]
        for i in range(4,16):
            new_key[i] = new_key[i-4] ^ key[i]
        return new_key
    
    check_dim2(key, 16, 8)
    expanded_key = []
    expanded_key.append(key)
    for i in range(1,11):
        key = update(key, i)
        expanded_key.append(key)
    return expanded_key

def aes_128(block, expanded_key):
    check_dim2(block, 16, 8)
    check_dim3(expanded_key, 11, 16, 8)
    
    state = add_round_key(block, expanded_key[0])
    for r in range(1,10):
        state = sub_bytes(state)
        state = shift_rows(state)
        state = mix_columns(state)
        state = add_round_key(state, expanded_key[r])
    
    state = sub_bytes(state)
    state = shift_rows(state)
    state = add_round_key(state, expanded_key[10])
    
    return state

def compose_bit(bits):
    assert 8 == len(bits)
    return sum([b.proj([0, 1 << i],8) for i,b in enumerate(bits)])

def compose_block(block):
    assert len(block) == 16
    return [compose_bit(c) for c in block]